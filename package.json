{
  "name": "cutemanga.moe",
  "version": "1.0.0",
  "description": "Cutemanga is a website where you can read manga with selectable text (OCR), it is a fun way to study japanese.",
  "main": "server.ts",
  "scripts": {
    "start": "npm run clean && rsbuild build && node server.ts",
    "build": "npm run clean && rsbuild build && npm run redirects",
    "redirects": "echo \"/* /index.html 200\" | cat > dist/_redirects",
    "server": "rsbuild build && npm run pm2",
    "pm2": "pm2 stop all && pm2 start server.ts",
    "clean": "del-cli ./dist"
  },
  "keywords": [
    "cute",
    "manga",
    "ocr",
    "moe",
    "anime",
    "kawaii",
    "learn japanese",
    "japanese learning",
    "cutemanga",
    "cuteanime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Moebits/Cutemanga.moe"
  },
  "author": "Moebits",
  "license": "MIT",
  "devDependencies": {
    "@rsbuild/core": "^1.4.11",
